    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-class-property": "^1.1.0",
